#include <iostream>
using namespace std;
#include <cmath> 
int main() {
    int n;
    bool sadedir = true;
    cout << "Bir ədəd daxil edin: ";
    cin >> n;
    if (n <= 1) {
        sadedir = false;
    } else {
        for (int i = 2; i <= std::sqrt(n); ++i) {
            if (n % i == 0) {
                sadedir = false;
                break; 
            }
        }
    }

    if (sadedir) {
      cout << n << " ədədi SADƏDİR." << endl;
    } else {
      cout << n << " ədədi SADƏ DEYİL." << endl;
    }
    return 0;
}
