#include <iostream>
using namespace std;
int main() {
    cout << "1-dən 10-a qədər vurma cədvəli:" << endl;
    for (int i = 1; i <= 10; ++i) {
        cout << i << " üçün vurma cədvəli:" <<endl;
        for (int j = 1; j <= 10; ++j) {
            cout << i << " x " << j << " = " << i * j << endl;
        }
        cout << "---" <<endl;
    }
    return 0;
}
